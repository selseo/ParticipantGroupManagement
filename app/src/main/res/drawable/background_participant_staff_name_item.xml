<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/colorDarkBrown"
                android:dashGap="20dp"/>
            <gradient
                android:angle="90"
                android:startColor="@color/colorDarkCream"
                android:endColor="@color/colorDarkCream"
                />

            <corners android:radius="10dp" />
        </shape>
    </item>
</selector>