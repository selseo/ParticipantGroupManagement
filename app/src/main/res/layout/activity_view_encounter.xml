<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_view_encounter"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background"
    tools:context="com.example.katesudal.participantgroupmanagement.Activity.ViewEncounterActivity">
    <com.example.katesudal.participantgroupmanagement.SortablePairEncounterTableView
        xmlns:table="http://schemas.android.com/apk/res-auto"
        android:id="@+id/tableViewEncounter"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="11"
        android:layout_marginBottom="10dp"
        table:tableView_headerColor="@color/colorDarkCream"
        table:tableView_columnCount="3" />
    <Button
        android:id="@+id/buttonBacktoMainFromViewEncounter"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/button_back_to_main"
        android:text="Back to Main">
    </Button>
</LinearLayout>
